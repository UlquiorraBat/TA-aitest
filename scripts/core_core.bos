FireWeapon2()
{
	if( gun_1 == 0 )
	{
		sleep 1000;
		turn blod_spindle_01 to z-axis <120.> speed <270.>;
	}
	if( gun_1 == 1 )
	{
		sleep 1000;
		turn blod_spindle_01 to z-axis <240.> speed <270.>;
	}
	if( gun_1 == 2 )
	{
		sleep 1000;
		turn blod_spindle_01 to z-axis <0.> speed <270.>;
	}
	++gun_1;
	if( gun_1 == 3 )
	{
		gun_1 = 0;
	}
}

